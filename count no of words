import java.util.Scanner;
class Solution{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        String str=scan.nextLine();
        long count=1;
        char ch,ch2;
        for(int i=0;i<str.length()-1;i++)
        {
          if(str.charAt(i)==' ' && str.charAt(i+1)!=' ')
            count++;
        }
        System.out.println(count);
        
    }
}
