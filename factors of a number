import java.util.Scanner;
class demo{
    int count=0;
    int demo(int n)
    {
        for(int i=1;i<=n;i++)
        {
            if(n%i==0)
            count++;
        }
        return count;
    }
}
class Solution{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n=scan.nextInt();
        demo ref=new demo();
        int result;
        result=ref.demo(n);
        System.out.println(result);
    }
}
