import java.util.Scanner;
class Solution{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int a=scan.nextInt();
        int b=scan.nextInt();
        int c=scan.nextInt();
        if(a>b && a>c)
         System.out.println(a);
        if(b>a &&b>a)
        System.out.println(b);
        else
        System.out.println(c);
    }
}
