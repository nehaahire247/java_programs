import java.util.Scanner;
class Solution{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int x=scan.nextInt();
        int y=scan.nextInt();
        int gcd=1;
        for(int i=1;i<=x&&i<=y;i++)
        {
            if(x%i==0 && y%i==0)
            gcd=i;
        }
        System.out.println(gcd);
    }
}
