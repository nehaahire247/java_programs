import java.util.Scanner;
class Solution{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        String str=scan.nextLine();
        int count=0;
        char ch;
        for(int i=0;i<str.length();i++)
        {   ch=str.charAt(i);
            if(ch>='0' && ch<='9')
            count++;
        }
        System.out.println(count);
    }
}
