//programe to print no of element of elements
import java.util.Scanner;
class Solution{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n=scan.nextInt();
        int a[]=new int[n];
        int count=0;
        for(int i=0;i<n;i++)
        {
            a[i]=scan.nextInt();
            count++;
        }
        System.out.println(count);
    }   
}
