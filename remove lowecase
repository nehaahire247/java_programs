import java.util.Scanner;
class Solution{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        String str=scan.nextLine();
        str=str+'\0';
        char ch[]=str.toCharArray();
        for(int i=0;ch[i]!='\0';i++)
        {
            if(ch[i]>='a' && ch[i]<='z')
            {
                continue;
            }
            else
            System.out.print(ch[i]);
        }
    }
}
