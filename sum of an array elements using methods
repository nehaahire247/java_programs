import java.util.Scanner;
class Demo{
    long sum=0;
    public long aVeryBigSum(int [] a){
        for(int i=0;i<a.length;i++)
        {
            sum=sum+a[i];
        }
        return sum;
    }
}
class Solution{
    public static void main(String[] args){
        Scanner scan = new Scanner(System.in);
        int n=scan.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=scan.nextInt();
        }
        long result;
        Demo m=new Demo();
        result=m.aVeryBigSum(a);
        
        System.out.println(result);
        
        
    }
}
